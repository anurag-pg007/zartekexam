      "source": [
        "def indexSum(nums,target):\n",
        "  has={}\n",
        "  for i,j in enumerate(nums):\n",
        "    k = target - nums[i]\n",
        "    if k in has:\n",
        "      return [has[k], i]\n",
        "    has[j]=i\n",
        "    \n",
        "nums = [3,2,4]\n",
        "target=6\n",
        "indexSum(nums,target)\n",
        "\n"
      ]
    }
  ]
